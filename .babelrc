{
  "presets": [
    "react-native"
  ],
  "plugins": [
    ["babel-plugin-root-import", {
      "paths": [{
        "rootPathPrefix": "@", 
        "rootPathSuffix": "app/components"
      }]
    }]
  ],
  "env": { 
    "production": {
      "plugins": [
        ["babel-plugin-root-import", {
          "rootPathPrefix": "@", 
          "rootPathSuffix": "app/components"
        }]
      ]
    }
  }
}